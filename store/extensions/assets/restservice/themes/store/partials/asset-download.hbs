<div class='table-responsive'>
    <div class='col-lg-12 row'>
        <div class='es-col-lg-12 col-lg-12'>
            {{#if downloadMetaData.downloadFileType}}
                <h4>{{downloadMetaData.downloadFileType}}</h4>
                <hr>
            {{else}}
                {{#if wadlContent}}
                    <h4>{{t "WADL"}}</h4>
                    <hr>
                {{else}}
                    {{#if swaggerContent}}
                        <h4>{{t "SWAGGER"}}</h4>
                        <hr>
                    {{/if}}
                {{/if}}
            {{/if}}
        </div>
    </div>
</div>
